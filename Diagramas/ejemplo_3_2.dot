digraph Diagrama_3_3 {
    graph [fontname="Arial"];
    node [fontname="Arial", shape=ellipse, style=solid];
    edge [fontname="Arial"];

    inicio [label="INICIO"];
    fin [label="FIN"];
    
    node [shape=rect, style=solid];
    init_nom [label="NOMINA \L\L- 0"];
    init_i [label="I \L\L- 1"];
    proceso_nom [label="NOMINA \L\L- NOMINA + SUE"];
    increment_i [label="I \L\L- I + 1"];
    
    node [shape=parallelogram, style=solid];
    lectura [label="SUE"];
    
    node [shape=egg, style="solid,waves", waves=bottom];
    escritura [label="NOMINA"];
    
    node [shape=diamond, style=solid];
    cond [label="I <= 10"];
    
    inicio -> init_nom -> init_i -> cond;
    cond -> lectura [label="Si"]; 
    lectura -> proceso_nom -> increment_i -> cond;
    cond -> escritura [label="No"];
    escritura -> fin;
}


