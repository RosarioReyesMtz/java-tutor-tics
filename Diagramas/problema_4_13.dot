    init_i_swap [label="I \L\L- 1"];
    init_j_swap [label="J \L\L- 1"];
    swap_logic [label="AUX \L\L- A[I, J]\lA[I, J] \L\L- A[N - I + 1, J]\lA[N - I + 1, J] \L\L- AUX\l"];
