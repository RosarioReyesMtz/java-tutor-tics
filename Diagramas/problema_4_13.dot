    swap_logic -> inc_j_swap -> cond_j_swap;
    cond_j_swap -> inc_i_swap [label="No"];
    inc_i_swap -> cond_i_swap;
    
    cond_i_swap -> init_i_write [label="No"];
    init_i_write -> cond_i_write;
    cond_i_write -> init_j_write [label="Si"];
    init_j_write -> cond_j_write;
    cond_j_write -> escritura_a [label="Si"];
    escritura_a -> inc_j_write -> cond_j_write;
    cond_j_write -> inc_i_write [label="No"];
    inc_i_write -> cond_i_write;
