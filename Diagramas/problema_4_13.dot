    cond_i_read [label="I <= N"];
    cond_j_read [label="J <= N"];
    cond_i_swap [label="I <= N / 2"];
    cond_j_swap [label="J <= N"];
    cond_i_write [label="I <= N"];
    cond_j_write [label="J <= N"];
    
    inicio -> lectura_n -> cond_n;
    cond_n -> init_i_read [label="Si"];
    cond_n -> fin [label="No\l\"Error en datos\"\l"];
    
    init_i_read -> cond_i_read;
